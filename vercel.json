{
  "version": 2,
  "builds": [
    {
      "src": "mmr_calc.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mmr_calc.py"
    }
  ],
  "functions": {
    "mmr_calc.py": {
      "runtime": "python3.9"
    }
  }
}
